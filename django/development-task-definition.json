{
  "family": "ecs-byfs-development-task-definition",
  "executionRoleArn": "arn:aws:iam::869061964712:role/iam-role-byfs-ecs-execution",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "django",
      "image": "ghilbut/byfs",
      "essential": true,
      "portMappings": [
        {
          "protocol": "tcp",
          "containerPort": 8000
        }
      ],
      "entryPoint": ["./manage.py", "runserver", "0:8000"],
      "environment": [{"name":"DJANGO_SETTINGS_MODULE","value":"byfs.settings.develop"}],
      "secrets": [{"name":"BYFS_DB_DEVELOP_HOST","valueFrom":"arn:aws:secretsmanager:ap-northeast-2:869061964712:secret:byfs-development-mysql-host-vz3LKC"},{"name":"BYFS_DB_DEVELOP_PORT","valueFrom":"arn:aws:secretsmanager:ap-northeast-2:869061964712:secret:byfs-development-mysql-port-eyuJ1e"},{"name":"BYFS_DB_DEVELOP_NAME","valueFrom":"arn:aws:secretsmanager:ap-northeast-2:869061964712:secret:byfs-development-mysql-name-JCXVCf"},{"name":"BYFS_DB_DEVELOP_USER","valueFrom":"arn:aws:secretsmanager:ap-northeast-2:869061964712:secret:byfs-development-mysql-username-Bwmd8c"},{"name":"BYFS_DB_DEVELOP_PASSWORD","valueFrom":"arn:aws:secretsmanager:ap-northeast-2:869061964712:secret:byfs-development-mysql-password-1qAaea"}]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}
