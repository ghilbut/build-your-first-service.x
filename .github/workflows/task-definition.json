{
  "family": "ecs-byfs-development-task-definition",
  "executionRoleArn": "arn:aws:iam::869061964712:role/iam-role-byfs-ecs-execution",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "django",
      "image": "ghilbut/byfs",
      "essential": true,
      "portMappings": [
        {
          "protocol": "tcp",
          "containerPort": 80
        }
      ],
      "entryPoint": ["./manage.py", "runserver", "0:80"],
      "environment": [
        {
          "name": "DJANGO_SETTINGS_MODULE",
          "value": "byfs.settings.local"
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}
