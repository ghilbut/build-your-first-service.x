{
  "familiy": "ecs-byfs-development-task-definition",
  "executionRoleArn": "arn:aws:iam::869061964712:role/iam-role-byfs-ecs-execution",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "django",
      "image": "ghilbut/byfs",
      "essential": true,
      "portMappings": [
        {
          "protocol": "tcp",
          "containerPort": 8000
        }
      ],
      "entryPoint": ["./manage.py", "runserver", "0:8000"]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}
